<div class='container'>
 <div class='row'>
   <h1>商品一覧</h1>
   <div class='container'>
    <div class='row'>
    <% @items.each do |item| %>
     <div class="col-lg-3">
      <%= link_to public_item_path(item.id) do %><%= attachment_image_tag item, :image,size: '60x60' %><% end %>
      <p><%= item.name %></p>
      <p>¥<%= (item.price * 1.1).round %></p>
     </div>
    <% end %>
    </div>
   </div>
  </div>
 </div>
</div>



